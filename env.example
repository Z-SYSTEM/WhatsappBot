# Configuración obligatoria del bot
BOT_NAME=MiBot

# Puerto del servidor (API REST + Interfaz Web)
PORT=4002
TOKENACCESS=tu_token_secreto_aqui

# Webhooks
ONMESSAGE=https://tu-servidor.com/webhook/message

# Health Check
HEALTH_CHECK_INTERVAL_SECONDS=30

# Aceptar llamadas (TRUE para aceptar, FALSE para rechazar automáticamente)
ACCEPT_CALL=FALSE

# Credenciales de la interfaz web
WEB_USER=admin
WEB_PASSWORD=admin
SESSION_SECRET=default_secret_key